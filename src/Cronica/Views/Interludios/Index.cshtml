@model IEnumerable<Interludio>

@{
    ViewData["Title"] = "Interludios";
}

<h2>@ViewData["Title"]</h2>

<p>
    <a asp-action="Create">Nuevo Interludio</a>
</p>
<table class="table">
    <thead>
    <tr>
    <th></th>
        <th>
            @Html.DisplayNameFor(model => model.FechaPrevista)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaResolucion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actual)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resuelto)
        </th>
    </tr>
    </thead>
    
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            <a asp-action="Edit" asp-route-id="@item.InterludioId">Editar</a> |
            <a asp-action="Delete" asp-route-id="@item.InterludioId">Borrar</a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaPrevista)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaResolucion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actual)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resuelto)
        </td>
    </tr>
}
</tbody>
</table>
